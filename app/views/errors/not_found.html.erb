 <!-- Content Wrapper. Contains page content -->
<div class="content-wrapper" data-turbolinks-permanent>
	<div class="row">
		<div class="col s4 offset-s2 error-container">
			<h3>Error</h3>
			<div class="error-code">
				404
			</div>
			<p class="error-message">La página que busca no existe</p>		
		</div>
		<div class="col s6">
			<h5>¿Que prefiere hacer?</h5>
			<ul>
				<li class="error-link">
					<a class="btn btn-submit" href="<%= request.env["HTTP_REFERER"] %>"><i class="fa fa-arrow-left" aria-hidden="true" data-turbolinks="false" ></i> Volver atrás</a>
				</li>
				<li class="error-link">				
					<a class="btn btn-submit" href="<%= root_path %>" data-turbolinks="false"><i class="fa fa-home" aria-hidden="true"></i> Ir al inicio</a>
				</li>
			</ul>
		</div>
	</div>
</div>