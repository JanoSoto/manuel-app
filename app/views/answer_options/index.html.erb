<p id="notice"><%= notice %></p>

<h1>Listing Answer Options</h1>

<table>
  <thead>
    <tr>
      <th>Answer</th>
      <th>Score</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_options.each do |answer_option| %>
      <tr>
        <td><%= answer_option.answer %></td>
        <td><%= answer_option.score %></td>
        <td><%= answer_option.question %></td>
        <td><%= link_to 'Show', answer_option %></td>
        <td><%= link_to 'Edit', edit_answer_option_path(answer_option) %></td>
        <td><%= link_to 'Destroy', answer_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer option', new_answer_option_path %>
